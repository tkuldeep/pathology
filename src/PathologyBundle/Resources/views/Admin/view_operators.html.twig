{% extends 'PathologyBundle::admin_dashboard.html.twig' %}

{% block title %}List of Operators{% endblock %}

{% block body %}

    <table border=1>
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Username</th>
            <th>Email</th>
            <th>Phone Number</th>
        </tr>
        {% for operator in operators %}
            <tr>
                <td>{{ operator.fname }}</td>
                <td>{{ operator.lname }}</td>
                <td>{{ operator.username }}</td>
                <td>{{ operator.email }}</td>
                <td>{{ operator.phoneNumber }}</td>
            </tr>
        {% endfor %}
    </table>


{% endblock %}




